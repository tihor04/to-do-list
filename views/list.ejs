<%- include("header") -%>


    <div class="box" id="heading">
      <h1><%=kindofday%></h1>
    </div>
    
    <div class="box">
      
        <% newitems.forEach(function(element){ %>
          <form action="/delete" method="post">
          <div class="item">
            <input type="checkbox" onchange="this.form.submit()" name="checkBox" value="<%= element._id %>">
            <p><%= element.name%></p>
            <input type="hidden" name="listname" value="<%=kindofday%>"></input>
          </div>
        </form>

       <% }) %>
        

      <form  class="item" action="/" method="post">
        <input type="text" name="entry" placeholder="Enter Task Here" autocomplete="off" />
        <button type="submit" name="submit" value=<%=kindofday%>>+</button>
      </form>
      
    </div>


    <%- include("footer") -%>
    

   
